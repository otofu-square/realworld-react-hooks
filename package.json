{
  "name": "realworld-react-hooks",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./src/.next && next build ./src",
    "start": "(cd ./src && node ./server.js)",
    "start:dev": "yarn start",
    "start:prod": "yarn build && NODE_ENV=production yarn start",
    "lint": "run-p eslint tsc stylelint",
    "eslint": "eslint --cache './**/*.{ts,tsx}'",
    "tsc": "tsc --noEmit",
    "stylelint": "NODE_ENV=stylelint stylelint --cache --syntax css-in-js './src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@emotion/core": "10.0.10",
    "@emotion/styled": "10.0.10",
    "express": "4.16.4",
    "isomorphic-unfetch": "3.0.0",
    "next": "8.1.0",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@types/express": "4.16.1",
    "@types/next": "8.0.3",
    "@types/react": "16.8.14",
    "@types/styled-components": "4.1.14",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@zeit/next-typescript": "1.1.1",
    "babel-plugin-emotion": "10.0.9",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.6.0",
    "npm-run-all": "4.1.5",
    "stylelint": "10.0.1",
    "stylelint-config-prettier": "5.1.0",
    "stylelint-config-recess-order": "2.0.2",
    "stylelint-config-standard": "18.3.0",
    "ts-node": "8.1.0",
    "typescript": "3.4.4"
  }
}
